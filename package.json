{
    "name": "ui",
    "version": "1.0.0",
    "description": "UI Server",
    "main": "index.js",
    "scripts": {
        "#start": "starts the express server",
        "start": "node dist/server.js",
        "#watch": "recompile for changes in development mode",
        "watch": "webpack --watch",
        "#compile": "compile the code for production",
        "compile": "webpack --mode production",
        "#lint": "lint for standard code practices",
        "lint": "eslint src browser server --ext js,jsx",
        "#watch-server-hmr": "recompile the server hmr bundle for changes",
        "watch-server-hmr": "webpack -w --config webpack.serverHMR.js",
        "dev-all": "rm dist/* && npm run watch-server-hmr & sleep 5 && npm start",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Vignesh",
    "license": "ISC",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^3.4.1",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "http-proxy-middleware": "^0.21.0",
        "isomorphic-fetch": "^2.2.1",
        "nodemon": "^1.18.9",
        "prop-types": "^15.8.1",
        "react": "^16.14.0",
        "react-bootstrap": "^0.33.1",
        "react-dom": "^16.14.0",
        "react-router-bootstrap": "^0.24.4",
        "react-router-dom": "^4.3.1",
        "react-select": "^2.4.4",
        "serialize-javascript": "^1.9.1",
        "url-search-params": "^1.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.3.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.33.2",
        "source-map-support": "^0.5.21",
        "webpack": "^4.47.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-middleware": "^3.7.3",
        "webpack-hot-middleware": "^2.26.1",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
